#!/usr/bin/env python2
# EASY-INSTALL-ENTRY-SCRIPT: 'pip==20.0.2','console_scripts','pip2.7'
__requires__ = 'pip==20.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pip==20.0.2', 'console_scripts', 'pip2.7')()
    )
