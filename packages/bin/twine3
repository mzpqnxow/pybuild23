#!/usr/bin/env python3
# EASY-INSTALL-ENTRY-SCRIPT: 'twine==1.11.0','console_scripts','twine'
__requires__ = 'twine==1.11.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('twine==1.11.0', 'console_scripts', 'twine')()
    )
